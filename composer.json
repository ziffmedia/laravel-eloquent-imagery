{
    "name": "ziffmedia/laravel-eloquent-imagery",
    "description": "Image handling per column/attribute for Laravel Eloquent",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Ralph Schindler",
            "email": "ralph.schindler@ziffmedia.com"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0",
        "phpunit/phpunit": "^9.0",
        "laravel/pint": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "ZiffMedia\\LaravelEloquentImagery\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "ZiffMedia\\LaravelEloquentImagery\\Test\\": "tests/"
        }
    },
    "suggest": {
        "ext-imagick": "Imagick image modifications",
        "ext-gd": "GD Based image modifications",
        "laravel/framework": "This package is intended to be used with laravel/framework version >= 5.7"
    },
    "conflict": {
        "laravel/framework": "<5.7.0",
        "laravel/nova": "<2.0.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "ZiffMedia\\LaravelEloquentImagery\\EloquentImageryProvider"
            ]
        }
    }
}
